{
  "expo": {
    "name": "Haven",
    "slug": "bookingApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "bookingapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "jsEngine": "hermes",
      "bundleIdentifier": "com.rizaldinp.bookingApp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "jsEngine": "hermes",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#1B563A"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.rizaldinp.bookingApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2b6140b8-736a-48b3-a58c-5319fe4773f8"
      }
    },
    "owner": "rizaldinp",
    "plugins": [
      "expo-router"
    ]
  }
}
